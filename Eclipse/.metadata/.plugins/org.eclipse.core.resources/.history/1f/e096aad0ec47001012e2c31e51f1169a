public class Exemplo011 {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
				byte by = 127; //-128 a 127
				by++;
				System.out.println(by);
				char letra = "A";
				letra += 3;
				System.out.println(Letra);
				if ('A' > 'a') { //a = 97
					System.out.println("A é maior que a");	
				} else {
					System.out.println("a é maior que A");
				
				}
				
	}
		
}
